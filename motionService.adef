sandboxed: false
version: 3.0.0
start: auto

executables:
{
  motionService = ( motionMonitor )
}

processes:
{
  envVars:
  {
    LE_LOG_LEVEL = DEBUG
  }
  run:
  {
     ( motionService )
  }
  faultAction: restartApp
}

bindings:
{

  motionService.motionMonitor.interrupt -> gpioService.le_gpioPin23
}

extern:
{
  motionService.motionMonitor.brnkl_motion
}
